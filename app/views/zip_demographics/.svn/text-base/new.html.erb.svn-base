<h1>New zip</h1>

<% form_for(@zip) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </p>
  <p>
    <%= f.label :total_population %><br />
    <%= f.text_field :total_population %>
  </p>
  <p>
    <%= f.label :total_housing_units %><br />
    <%= f.text_field :total_housing_units %>
  </p>
  <p>
    <%= f.label :land_area_square_miles %><br />
    <%= f.text_field :land_area_square_miles %>
  </p>
  <p>
    <%= f.label :water_area_square_miles %><br />
    <%= f.text_field :water_area_square_miles %>
  </p>
  <p>
    <%= f.label :lat %><br />
    <%= f.text_field :lat %>
  </p>
  <p>
    <%= f.label :lng %><br />
    <%= f.text_field :lng %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', zips_path %>