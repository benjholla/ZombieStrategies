<h1>Listing zips</h1>

<table>
  <tr>
    <th>Zip</th>
    <th>Total population</th>
    <th>Total housing units</th>
    <th>Land area square miles</th>
    <th>Water area square miles</th>
    <th>Lat</th>
    <th>Lng</th>
  </tr>

<% @zips.each do |zip| %>
  <tr>
    <td><%=h zip.zip %></td>
    <td><%=h zip.total_population %></td>
    <td><%=h zip.total_housing_units %></td>
    <td><%=h zip.land_area_square_miles %></td>
    <td><%=h zip.water_area_square_miles %></td>
    <td><%=h zip.lat %></td>
    <td><%=h zip.lng %></td>
    <td><%= link_to 'Show', zip %></td>
    <td><%= link_to 'Edit', edit_zip_path(zip) %></td>
    <td><%= link_to 'Destroy', zip, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New zip', new_zip_path %>