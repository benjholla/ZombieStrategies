<h1>Listing product_location_memberships</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Location</th>
    <th>Created at</th>
  </tr>

<% @product_location_memberships.each do |product_location_membership| %>
  <tr>
    <td><%=h product_location_membership.product_id %></td>
    <td><%=h product_location_membership.location_id %></td>
    <td><%=h product_location_membership.created_at %></td>
    <td><%= link_to 'Show', product_location_membership %></td>
    <td><%= link_to 'Edit', edit_product_location_membership_path(product_location_membership) %></td>
    <td><%= link_to 'Destroy', product_location_membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_location_membership', new_product_location_membership_path %>