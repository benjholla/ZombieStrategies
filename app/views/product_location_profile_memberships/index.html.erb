<h1>Listing product_location_profile_memberships</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Location profile</th>
    <th>Created at</th>
  </tr>

<% @product_location_profile_memberships.each do |product_location_profile_membership| %>
  <tr>
    <td><%=h product_location_profile_membership.product_id %></td>
    <td><%=h product_location_profile_membership.location_profile_id %></td>
    <td><%=h product_location_profile_membership.created_at %></td>
    <td><%= link_to 'Show', product_location_profile_membership %></td>
    <td><%= link_to 'Edit', edit_product_location_profile_membership_path(product_location_profile_membership) %></td>
    <td><%= link_to 'Destroy', product_location_profile_membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

